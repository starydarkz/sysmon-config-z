<Sysmon schemaversion="0.1">
  <EventFiltering>
    <RuleGroup name="" groupRelation="or">
      <FileCreate onmatch="include">
        <TargetFilename condition="begin with">C:\Users\Default</TargetFilename>        <!--Microsoft:Windows: Changes to default user profile-->
      </FileCreate>
    </RuleGroup>
  </EventFiltering>
</Sysmon>