<Sysmon schemaversion="0.1">
    <EventFiltering>
        <RuleGroup name="" groupRelation="or">
            <RegistryEvent onmatch="exclude">
                <!--SentinelOne:Agent-->
                <Image condition="begin with">C:\Program Files\SentinelOne\Sentinel Agent</Image>
            </RegistryEvent>
        </RuleGroup>
    </EventFiltering>
</Sysmon>