<Sysmon schemaversion="0.1">
   <EventFiltering>
      <RuleGroup name="" groupRelation="or">
         <FileCreateStreamHash onmatch="include">
            <TargetFilename condition="contains">Temp\7z</TargetFilename>            <!--7zip extractions-->
         </FileCreateStreamHash>
      </RuleGroup>
   </EventFiltering>
</Sysmon>